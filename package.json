{
  "name": "grapesjs-mjml",
  "version": "0.0.26",
  "description": "MJML Componenets integration in GrapesJS",
  "main": "dist/grapesjs-mjml.min.js",
  "scripts": {
    "lint": "eslint src",
    "build": "cross-env WEBPACK_ENV=prod && npm run v:patch && webpack",
    "v:patch": "npm version --no-git-tag-version patch",
    "start": "cross-env WEBPACK_ENV=dev ./node_modules/.bin/webpack-dev-server --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-mjml.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "mjml",
    "wysiwyg"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": [
            "> 1%",
            "ie 11",
            "safari 8"
          ],
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.1.1",
    "node-sass": "^4.5.3",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "mjml": "^3.3.5"
  }
}
